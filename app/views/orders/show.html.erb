<div id="customer-info">
  Customer Information
  <p>Name: <%= @customer.name %> </p>
  <p>Address: <br />
    <%= @customer.street %><br />
    <%= @customer.city %> <%= @customer.state %> <%= @customer.zip %>
  </p>
</div>
<% @items.each do |item| %>
  <div id="item-<%= item.id %>">
    <ul>
      <li><%= item.name %></li>
    </ul>
  </div>
<% end %>