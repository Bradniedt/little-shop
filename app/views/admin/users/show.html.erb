<%= @user.name %>
<%= @user.street %>
<%= @user.city %>
<%= @user.state %>
<%= @user.zip %>
<%= @user.email %>


<% if @user_role == 'default' %>
  <%= link_to 'Edit Profile', profile_edit_path %>
  <%= link_to 'Upgrade to Merchant', admin_merchant_path(@user, :upgrade => true), method: :put %>
<% elsif @user_role == 'merchant' %>
  <%= link_to 'My Items', dashboard_items_path %>
<% end %>
