
<%= form_for [:admin, @user] do |f| %>
  <ul>
  <% if @errors %>
    <div class="flash">
      <% @errors.full_messages.each do |error| %>
        <li><%= error %></li>
      <% end %>
    </div>
  <% end %>
  </ul>
  <ul class="registration-list">
    <li><%= f.label :name %></li>
    <li><%= f.text_field :name %></li>
    <li><%= f.label :street %></li>
    <li><%= f.text_field :street %></li>
    <li><%= f.label :city %></li>
    <li><%= f.text_field :city %></li>
    <li><%= f.label :state %></li>
    <li><%= f.text_field :state %></li>
    <li><%= f.label :zip %></li>
    <li><%= f.text_field :zip %></li>
    <li><%= f.label :email %></li>
    <li><%= f.text_field :email %></li>
    <li><%= f.label :password %></li>
    <li><%= f.password_field :password %></li>
    <li><%= f.label :password_confirmation %></li>
    <li><%= f.password_field :password_confirmation %></li>
    <li><%= f.submit 'Submit', id:"submit" %></li>
  </ul>
<% end %>
