<% @merchants.each do |merchant| %>
    <ul id="merchant-<%=merchant.id%>">
      <li><%= merchant.name %></li>
      <li><%= merchant.city %></li>
      <li><%= merchant.state %></li>
      <li><%= merchant.street %></li>
      <li><%= merchant.created_at %></li>
        <% if current_admin? %>
        <li><%= link_to("#{merchant.name}", admin_merchant_path(merchant.id)) %></li>
          <% if merchant.enabled %>
          <li><%= button_to "disable", "/deactivate", method: :enable, params: { user_id: merchant.id, enabled: false } %></li>
          <% else  %>
          <li><%= button_to "enable", "/activate", method: :enable, params: { user_id: merchant.id, enabled: true } %></li>
          <% end %>
        <% end %>
    </ul>
<% end %>
