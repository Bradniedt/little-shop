<% @merchants.each do |merchant| %>
    <ul id="merchant-<%=merchant.id%>">
      <li><%= merchant.name %></li>
      <li><%= merchant.city %></li>
      <li><%= merchant.state %></li>
      <li><%= merchant.created_at %></li>
      <% if current_admin? %>
      <%= link_to(admin_merchants_path(merchant.id)) %>
      <% end %>
    </ul>
<% end %>
